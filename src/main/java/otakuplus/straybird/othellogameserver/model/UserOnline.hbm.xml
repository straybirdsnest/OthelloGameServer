<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="otakuplus.straybird.othellogameserver.model">
	<class name="UserOnline" table="OthelloUserOnline">
		<id name="userId" type="int" column="user_id">
			<generator class="identity" />
		</id>
		<property name="onlineState" type="int" column="online_state" />
	</class>
</hibernate-mapping>
